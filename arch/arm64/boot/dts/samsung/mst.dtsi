#if !(1)
&${mst_pwr_en_parent} {
	mst_pwr_en: sec-mst,mst-pwr-gpio {
		pins_cmd_dat {
			pinmux = <MTK_PINMUX(SEC_GPIO_NUM(${mst_power_enable}), 0)>;
			output-low;
			bias-pull-down;
		};
	};
};
#endif

&pio {
	mst_en: sec-mst,mst-en-gpio {
		pins_cmd_dat {
			pinmux = <MTK_PINMUX(SEC_GPIO_NUM(AP,pio,23), 0)>;
			output-low;
			bias-pull-down;
		};
	};
};

&pio {
	mst_data: sec-mst,mst-data-gpio {
		pins_cmd_dat {
			pinmux = <MTK_PINMUX(SEC_GPIO_NUM(AP,pio,21), 0)>;
			output-low;
			bias-pull-down;
		};
	};
};

&smd {
	sec-mst{
		compatible = "sec-mst";
#if !(1)
		sec-mst,mst-pwr-gpio = <SEC_GPIO_REF(${mst_power_enable}) 0>;
#endif
		sec-mst,mst-en-gpio = <SEC_GPIO_REF(AP,pio,23) 0>;
		sec-mst,mst-data-gpio = <SEC_GPIO_REF(AP,pio,21) 0>;
#if 1
		sec-mst,mst-support-gpio = <SEC_GPIO_REF(AP,pio,78) 0>;
#endif

		pinctrl-names = "default";
		pinctrl-0 = <
#if !(1)
				&mst_pwr_en 
#endif
				&mst_en &mst_data>;
	};
};
